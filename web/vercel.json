{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.getsyncer.tech"
        }
      ],
      "destination": "https://getsyncer.tech/:1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:path((?!.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}
